%tr[teacher]{ :class => cycle("", "alt") }
  %td= link_to teacher.name, admin_teacher_path(teacher)
  %td= teacher.email
  %td= link_to("edit", edit_admin_teacher_path(teacher))
  %td= button_to("delete", admin_teacher_path(teacher), :confirm => "Are you sure?", :method => :delete, :class => "button delete")